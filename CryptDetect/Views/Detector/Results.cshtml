
@using CryptDetect.Models;
@model List<DataModel>;
@{
    ViewData["Title"] = "Results";
}
<link rel="stylesheet" href="~/css/circle.css" />
<div class="d-flex justify-content-center">

    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="145" height="145" viewBox="0 0 145 145">
        <image id="final" width="145" height="145" xlink:href="data:image/png;base64," />
    </svg>
    <br />
    <div class="pt-xl-5">
        <h3 style="color:white;">We have successfully scanned your code!</h3>
    </div>

</div>
<br />
<div class="container pt-5">
    <div class="row">
        @foreach (DataModel item in Model)
        {
            <div class="card bg-info mx-auto text-center" style="width: 13rem;">
                <div class="card-body">
                    <h3 class="card-title">@item.bType</h3>
                    <div class="d-flex justify-content-center">
                        <div class="circle">@item.chance</div>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
<br />

<button type="button" class="scanButton" onclick="download_txt()" id="downloadtxt">
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-text" viewBox="0 0 16 16">
        <path d="M5 4a.5.5 0 0 0 0 1h6a.5.5 0 0 0 0-1H5zm-.5 2.5A.5.5 0 0 1 5 6h6a.5.5 0 0 1 0 1H5a.5.5 0 0 1-.5-.5zM5 8a.5.5 0 0 0 0 1h6a.5.5 0 0 0 0-1H5zm0 2a.5.5 0 0 0 0 1h3a.5.5 0 0 0 0-1H5z" />
        <path d="M2 2a2 2 0 0 1 2-2h8a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V2zm10-1H4a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1z" />
    </svg>
    Download JSON
</button>

<script type="text/javascript">
    async function download_txt() {
        const data = new FormData();
        data.append('sessionID','gyhukkgj');
        const post_req = await fetch("http://127.0.0.1:8001/download", {
            method: 'POST',
            cache: 'no-cache',
            redirect: 'follow',
            referrerPolicy: 'no-referrer',
            body: data
        }).then(response => response.blob()).then(function (myBlob) {
            var test = window.URL.createObjectURL(myBlob);
            var a = document.createElement('a');
            a.src = test;
            a.href = test;
            a.download = 'test.json';
            document.body.appendChild(a);
            a.click();
            a.remove();
        });
    };
</script>